{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "string",
      "pattern": "^[a-f0-9]+$"
    },
    "segment": {
      "type": "object",
      "properties": {
        "segments": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "segment_ec": {
                "type": "array",
                "items": {
                  "type": "string",
                  "pattern": "^[a-f0-9]+$"
                }
              }
            },
            "required": ["segment_ec"]
          }
        }
      },
      "required": ["segments"]
    }
  },
  "required": ["data", "segment"]
}
